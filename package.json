{
  "name": "fast-task",
  "productName": "",
  "version": "0.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "electron ./dist",
    "webpackRender": "webpack --config webpack.render.js --mode development --watch",
    "webpackMain": "webpack --config webpack.main.js --mode development",
    "buildRender": "webpack --config webpack.render.js --mode production",
    "buildMain": "webpack --config webpack.main.js --mode production",
    "package": "npm run buildRender && npm run buildMain && electron-packager ./dist --icon=./assets/icon.icns"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "electron": "^2.0.0",
    "electron-packager": "^12.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.1"
  }
}
